# .env - Environment variables for MiniZinc project scripts

# Base directory of the project
export MINIZINC_PROJECT_ROOT="/data/data/com.termux/files/home/storage/github/libminizinc"

# Build directory for libminizinc
export LIBMINIZINC_BUILD_DIR="${MINIZINC_PROJECT_ROOT}/build"

# Gecode vendor directory
export GECODE_VENDOR_DIR="${MINIZINC_PROJECT_ROOT}/vendor/gecode"

# Gecode build directory
export GECODE_BUILD_DIR="${GECODE_VENDOR_DIR}/build"

# MiniZinc user solver configuration directory
export MINIZINC_USER_SOLVERS_DIR="/data/data/com.termux/files/home/.minizinc/solvers"

# Path to the MiniZinc model files
export MINIZINC_MODELS_DIR="${MINIZINC_PROJECT_ROOT}"

# Path to the MiniZinc data files
export MINIZINC_DATA_DIR="${MINIZINC_PROJECT_ROOT}"

# Path to the scripts directory
export MINIZINC_SCRIPTS_DIR="${MINIZINC_PROJECT_ROOT}/scripts"
