<http://solfunmeme.com/ontology/emoji_algebra>
  rdf:type owl:Ontology ;
  rdfs:comment "Ontology defining the algebraic meanings of emojis for dataset equivalence proofs." ;
  owl:imports <http://solfunmeme.com/ontology/emoji> ;
.


# Emoji Algebra Concepts

:AlgebraicConcept
  rdf:type owl:Class ;
  rdfs:label "Algebraic Concept" ;
  rdfs:comment "A fundamental concept in the emoji algebra, representing a mathematical or logical entity." ;
.

:Operation
  rdf:type owl:Class ;
  rdfs:label "Operation" ;
  rdfs:comment "An operation that transforms or relates Algebraic Concepts." ;
  rdfs:subClassOf :AlgebraicConcept ;
.

:Type
  rdf:type owl:Class ;
  rdfs:label "Type" ;
  rdfs:comment "A classification for Algebraic Concepts." ;
  rdfs:subClassOf :AlgebraicConcept ;
.

:Path
  rdf:type owl:Class ;
  rdfs:label "Path" ;
  rdfs:comment "A connection or transformation between two Algebraic Concepts, representing a proof step or homotopy." ;
  rdfs:subClassOf :AlgebraicConcept ;
.

:Equivalence
  rdf:type owl:Class ;
  rdfs:label "Equivalence" ;
  rdfs:comment "A special type of path indicating that two Algebraic Concepts are homotopically equivalent." ;
  rdfs:subClassOf :Path ;
.


